<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <h2>いいねリスト</h2>
      <table class="table">
      <% @user.likes.each do |like| %>
        <tr>
          <th>
            <%= link_to shop_path(like.shop) do %>
              <%= attachment_image_tag like.shop, :image, :fill, 300, 300, format: 'jpeg' %>
            <% end %>
          </th>
        </tr>
      <% end %>
      </table>
    </div>
  </div>
</div>